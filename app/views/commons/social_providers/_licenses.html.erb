<h2>Информация о лицензиях</h2>
<% if @social_provider.licenses.any? %>
  <table>
    <thead>
      <tr>
        <th>Серия</th>
        <th>Номер</th>
        <th>Дата выдачи</th>
        <th>Срок действия</th>
        <th>Вид деятельности</th>
        <th>Выдавшая организация</th>
      </tr>
    </thead>
    <tbody>
      <% @social_provider.licenses.each do |license| %>
        <tr>
          <td><%= license.seria %></td>
          <td><%= license.number %></td>
          <td><%= I18n.l(license.issue_date) %></td>
          <td><%= I18n.l(license.expiration_date) %></td>
          <td><%= license.activity_kind %></td>
          <td><%= license.issuing_organization %></td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% else %>
  <p>Нет информации</p>
<% end %>
